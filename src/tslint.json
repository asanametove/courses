{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "courses",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "courses",
            "kebab-case"
        ],
        "trailing-comma": [true, {"multiline": "always", "singleline": "never"}]
    }
}
